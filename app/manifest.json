{
  "name": "AWS SAML Keys",
  "version": "1.0.7",
  "manifest_version": 2,
  "description": "Generate AWS access keys from a SAML login to the AWS console.",
  "icons": { 
    "16": "img/key16.png",
    "48": "img/key48.png",
    "128": "img/key128.png"
  },
  "browser_action": {
    "default_popup": "menu.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "alarms",
    "https://signin.aws.amazon.com/saml",
    "https://*.signin.aws.amazon.com/saml",
    "nativeMessaging",
    "notifications",
    "storage",
    "webRequest"
  ]
}